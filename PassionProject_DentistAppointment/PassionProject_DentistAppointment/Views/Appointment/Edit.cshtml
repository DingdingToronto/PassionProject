﻿@model PassionProject_DentistAppointment.Models.AppointmentDto

@{
    ViewBag.Title = "Edit";
}

<h2>Edit Appointment</h2>

<form method="POST" action="/Appointment/Update/@Model.AppointmentID">
    <div>
        <label>ID</label>
        <input type="text" name="AppointmentID" value="@Model.AppointmentID" readonly />
    </div>
    <div>
        <label>Name</label>
        <input type="text" name="AppointmentDate" value="@Model.AppointmentDate" />
    </div>
   
    <div>


        <input type="submit" value="Update" />
    </div>

</form>

